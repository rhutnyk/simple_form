    {
      "id": "auditInfo",
      "blockTitle": "Audit Information",
      "questions": [
        {
          "text": "Place where audit occured?",
          "type": "radio",
          "options": [
            { "text": "Office" },
            { "text": "JobSite" }
          ],
          "value": "JobSite"
        },
        {
          "type": "radio",
          "text": "Audit type",
          "options": [
            { "text": "Random" },
            { "text": "New Truck" },
            { "text": "Termination" }
          ],
          "value": "Random"
        },
        {
          "type": "select",
          "text": "Who is performing the audit?",
          "resource": {
            "name": "auditor",
            "dataText": "Name",
            "dataValue": "EmplCode"
          },
          "default": { "storage": "auditor" },
          "validation": { "required": true },
          "value": "HARSTA"
        },
        {
          "text": "Technician Present?",
          "type": "radio",
          "options": [
            { "text": "Yes" },
            { "text": "No" }
          ],
          "childQuestions": {
            "dependency": "Yes",
            "questions": [
              {
                "type": "select",
                "text": "Technician's name",
                "resource": {
                  "name": "tech",
                  "dataText": "Name",
                  "dataValue": "EmplCode"
                },
                "notify": {
                  "event": "techchange",
                  "parameter": "tech"
                },
                "dependency": {
                  "data": {
                    "event": "truckchange",
                    "resource": "Employees",
                    "datafield": "EmplCode"
                  }
                },
                "value": "DINANT"
              }
            ]
          },
          "notify": { "event": "techpresentchange" },
          "value": "Yes"
        },
        {
          "type": "txt",
          "text": "Truck #",
          "default": { "storage": "subject" },
          "notify": {
            "event": "truckchange",
            "parameter": "VehicleNumber"
          },
          "validation": { "required": true },
          "value": "1243"
        }
      ],
      "workflow": {
        "status": "InProgress",
        "role": "auditor"
      },
      "state": {
        "visible": true,
        "enabled": true
      }
    },
